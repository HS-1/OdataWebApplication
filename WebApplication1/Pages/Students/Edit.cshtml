@page  
@model WebApplication1.Pages.Students.EditModel
@{
    ViewData["Title"] = "Edit";
}

<form method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label> Id</label>
        <input type="text" class="form-control col-md-3" asp-for="studentModel.Id" readonly="readonly" contenteditable="false" />
    </div>
    <div class="form-group">
        <label> Title</label>
        <input type="text" class="form-control col-md-3" asp-for="studentModel.Name" />
        <span asp-validation-for="studentModel.Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label> Title</label>
        <input type="text" class="form-control col-md-3" asp-for="studentModel.Score" />
        <span asp-validation-for="studentModel.Score" class="text-danger"></span>
    </div>

    <div>
        <button type="submit" asp-page-handler="UpdateApplication">Save</button>
        <button type="submit" asp-page-handler="Back">Back</button>
    </div>
</form>

